@article{WAWROWSKI20212259,
title = {Detecting anomalies and attacks in network traffic monitoring with classification methods and XAI-based explainability},
journal = {Procedia Computer Science},
volume = {192},
pages = {2259-2268},
year = {2021},
note = {Knowledge-Based and Intelligent Information \& Engineering Systems: Proceedings of the 25th International Conference KES2021},
issn = {1877-0509},
doi = {https://doi.org/10.1016/j.procs.2021.08.239},
url = {https://www.sciencedirect.com/science/article/pii/S1877050921017361},
author = {Łukasz Wawrowski and Marcin Michalak and Andrzej Białas and Rafał Kurianowicz and Marek Sikora and Mariusz Uchroński and Adrian Kajzer},
keywords = {anomaly detection, classification, network traffic security, explainable artificial intelligence;},
abstract = {Assuring the network traffic safety is a very important issue in a variety of today’s industries. Therefore, the development of anomalies and attacks detection methods has been the goal of analyses. In the paper the binary classification-based approach to network traffic safety monitoring is presented. The well known methods were applied to artificially modified network traffic data and their detection capabilities were tested. More detailed interpretation of the nature of detected anomalies is carried out with the help of the XAI approach. For the purpose of experiments a new benchmark network traffic data set was prepared, which is now commonly available.}
}

@article{TIAN201084,
title = {A review and prospect on the network congestion control},
journal = {The Journal of China Universities of Posts and Telecommunications},
volume = {17},
pages = {84-90},
year = {2010},
issn = {1005-8885},
doi = {https://doi.org/10.1016/S1005-8885(09)60601-4},
url = {https://www.sciencedirect.com/science/article/pii/S1005888509606014},
author = {Yan-tao TIAN and Ping-ping XIAO},
keywords = {congestion control, dynamics model, transmission control protocol (TCP), queue management, queue scheduling},
abstract = {Network congestion has become more critical due to the rapid increase in the total number of Internet users and applications. This paper gives a survey for the research development of network congestion control in the past twenty years. The evaluation index of the ideal congestion control protocol is introduced, such as stability, fairness, efficiency, etc. Several network models and analysis methods are also given for the development of the congestion control, such as the nonlinear model, the control theory, the optimization method. On the basis of massive research of the congestion control issues, there is the generalization of the end-to-end protocol and intermediate nodes. Some open problems have been discussed for the further research of network congestion control.}
}

