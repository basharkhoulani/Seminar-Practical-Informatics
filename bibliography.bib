@inproceedings{10.1145/3342280.3342305,
author = {Fu, Yongquan and Li, Dongsheng and Shen, Siqi and Zhang, Yiming and Chen, Kai},
title = {Resilient Disaggregated Network Flow Monitoring},
year = {2019},
isbn = {9781450368865},
